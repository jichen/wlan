<%@page contentType="text/html;charset=UTF-8"%>
<%@include file="/include.inc.jsp" %>
<form method="post" action="${ctx }/sysmanagement/customer/add" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
	<table border="1" align="left" style="width: 70%;height:100%;">
		<tr>
			<td align="left" height="25px">
				企业名称:<input type="text" name="cust_name"  class="required" size="20" maxlength="32" />
			</td>
		</tr>
		<tr>
			<td align="left" height="25px">
				地址:<input type="text" name="address"   class="required" size="20" maxlength="32"  />
			</td>
		</tr>
		<tr>
			<td align="left" height="25px">
				联系人:<input type="text" name="contact"   class="required" size="20" maxlength="32"  />
			</td>
		</tr>
		<tr>
			<td align="left" height="25px">
				联系方式:<input type="text" name="phone"  class="" size="20" maxlength="32"/>
			</td>
		</tr>
		<tr>
			<td align="left" height="25px"><input type="hidden"/>备注:</td>
		</tr>	
		<tr>
			<td align="left"> 
				<textarea cols="91" rows="4" name="remark"></textarea>
			</td>
		</tr>	
	
	</table>


			
	<div class="formBar">
		<ul>
			<li><div class="buttonActive"><div class="buttonContent"><button type="submit">确定</button></div></div></li>
			<li><div class="button"><div class="buttonContent"><button type="button" class="close">关闭</button></div></div></li>
		</ul>
	</div>
</table>
</form>
