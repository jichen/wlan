<%@page contentType="text/html;charset=UTF-8"%>
<%@include file="/include.inc.jsp" %>
<form method="post" action="${ctx }/sysmanagement/customer/add" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
	<div class="pageFormContent" layoutH="56">
		<div>
			<p>
				<label>企业名称：</label><input type="text" name="cust_name"  class="required" size="40" maxlength="200" />
			</p><p></p><p style="width: 50px"></p>
			<p>
				<label>地址：</label><input type="text" name="address" class="required" size="40" maxlength="200"  />
			</p><p></p><p style="width: 50px"></p>
			<p>
				<label>联系人：</label><input type="text" name="contact"   class="required" size="40" maxlength="100"  />
			</p><p></p><p style="width: 50px"></p>
			<p>
				<label>联系方式：</label><input type="text" name="phone"   class="required" size="40" maxlength="50"  />
			</p><p></p><p style="width: 50px"></p>
			<dl class="nowrap">
				<dt>备注：</dt>
				<dd><textarea name="remark" cols="80" maxlength="2000" rows="12" ></textarea>
				</dd>
			</dl>
		</div>
	</div>
	<div class="formBar">
		<ul>
			<li><div class="buttonActive"><div class="buttonContent"><button type="submit">确定</button></div></div></li>
			<li><div class="button"><div class="buttonContent"><button type="button" class="close">关闭</button></div></div></li>
		</ul>
	</div>
</form>
