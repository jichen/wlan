<%@page contentType="text/html;charset=UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>后台登录页面</title>
<script type="text/javascript">
</script>
</head>
<body>
<div id="login"  align="center">
    <div>后台登录</div>
    <div>
       <form action="<%=request.getContextPath() %>/login/submit" method="post">
          <div>
            <span>用户名：</span>
            <input id="usernameId" name="username" type="text" maxlength="20" value="" />
          </div>
          <div>
            <span>密&nbsp;&nbsp;码：</span>
            <input id="passwordId" name="password" type="password" maxlength="20" value="" />
          </div>
          <div><p/></div>
          <div align="center">
		    <input name="" type="submit" id="J_SubmitStatic" value="登录"/>
		    <input name="" type="reset" id="J_ResetStatic" value="重置"/>
  		  </div>
       </form>
    </div>
</div>
</body>
</html>